# If using fish shell this will correct the colors for the theme. 
# Just add the following lines to your fish.config 

set fish_color_normal default
set fish_color_command green --bold
set fish_color_quote yellow --bold
set fish_color_redirection blue --bold
set fish_color_end red
set fish_color_error red
set fish_color_param yellow
set fish_color_comment cyan --bold
set fish_color_match  cyan --bold
set fish_color_selection yellow --bold
set fish_color_search_match yellow --bold
set fish_color_operator cyan
set fish_color_escape red
set fish_color_cwd magenta --bold
set fish_color_autosuggestion cyan
set fish_color_user magenta
set fish_color_host magenta
set fish_color_cancel red